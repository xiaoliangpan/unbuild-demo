#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/vilin/vilin_object/unbuild-demo/node_modules/.pnpm/mkdist@1.4.0_typescript@5.3.3/node_modules/mkdist/dist/node_modules:/Users/vilin/vilin_object/unbuild-demo/node_modules/.pnpm/mkdist@1.4.0_typescript@5.3.3/node_modules/mkdist/node_modules:/Users/vilin/vilin_object/unbuild-demo/node_modules/.pnpm/mkdist@1.4.0_typescript@5.3.3/node_modules:/Users/vilin/vilin_object/unbuild-demo/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/vilin/vilin_object/unbuild-demo/node_modules/.pnpm/mkdist@1.4.0_typescript@5.3.3/node_modules/mkdist/dist/node_modules:/Users/vilin/vilin_object/unbuild-demo/node_modules/.pnpm/mkdist@1.4.0_typescript@5.3.3/node_modules/mkdist/node_modules:/Users/vilin/vilin_object/unbuild-demo/node_modules/.pnpm/mkdist@1.4.0_typescript@5.3.3/node_modules:/Users/vilin/vilin_object/unbuild-demo/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../dist/cli.cjs" "$@"
fi
