{
  "name": "unbuild-demo",
  "version": "1.0.1-beta.1",
  "description": "description",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "unbuild",
    "build:deno": "udeno",
    "build:esm": "mkdist -d --dist=dist/es",
    "build:cjs": "mkdist -d --dist=dist/lib --format=cjs",
    "dev": "unbuild --watch --stub",
    "clean": "rimraf dist",
    "play": "tsx src/index.tsx",
    "deps:fresh:major": "npx taze major -w",
    "deps:fresh:minor": "npx taze minor -w",
    "deps:fresh:patch": "npx taze patch -w",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --tag --push && pnpm publish"
  },
  "keywords": [
    "utils"
  ],
  "files": [
    "dist"
  ],
  "author": "Vilin",
  "license": "MIT",
  "devDependencies": {
    "bumpp": "^9.3.0",
    "husky": "^9.0.6",
    "mkdist": "^1.4.0",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.0",
    "udeno": "^0.3.6",
    "unbuild": "^2.0.0"
  }
}